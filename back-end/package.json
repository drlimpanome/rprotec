    "dev": "nodemon src/app.ts",
    "build": "tsc && mkdir -p dist/config && cp src/config/database.js dist/config/database.js",
    "start": "node dist/app.js",
    "db:migrate": "ts-node -r tsconfig-paths/register ./node_modules/.bin/sequelize-cli db:migrate",
    "db:migrate:undo": "ts-node -r tsconfig-paths/register ./node_modules/.bin/sequelize-cli db:migrate:undo",
    "db:seed": "ts-node -r tsconfig-paths/register ./node_modules/.bin/sequelize-cli db:seed:all"
  },
